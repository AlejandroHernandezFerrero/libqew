<project name="libqewplugin" default="netbeans" basedir=".">
    <description>Builds, tests, and runs the project libqewplugin.</description>
    <import file="nbproject/build-impl.xml"/>

    <target name="release">
        <copy todir="${cluster}/modules/ext"
              file="release/modules/ext/libqew.jar"/>
        <copy todir="${cluster}/modules/ext/sources"
              file="release/modules/ext/sources/libqew-sources.zip"/>
        <copy todir="${cluster}/modules/ext/docs"
              file="release/modules/ext/docs/libqew-javadoc.zip"/>
    </target>

</project>
